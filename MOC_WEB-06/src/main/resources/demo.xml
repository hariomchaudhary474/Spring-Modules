<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="jsp"
		class="org.springframework.web.servlet.view.InternalResourceViewResolver">
		<property name="prefix" value="/pages/" />
		<property name="suffix" value=".jsp" />
	</bean>
	<bean id="/form" class="com.maersk.line.web.DisplayFormController" />
	<bean id="/welcome" class="com.maersk.line.web.WelcomeController" />
	<bean id="/hello" class="com.maersk.line.web.HelloController" />
	<bean id="/address" class="com.maersk.line.web.AddressController" />
</beans>
